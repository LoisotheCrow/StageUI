{
    "name": "@stage-ui/core",
    "author": "Research technology",
    "version": "0.2.0-beta9",
    "license": "MIT",
    "scripts": {
        "build": "tsc && yarn build:js && cp package.json README.md ../../build/core/ && node ../../scripts/buildConfig core",
        "build:js": "babel ./ --out-dir ../../build/core --extensions \".ts,.tsx\" --ignore \"node_modules/\"",
        "type-check": "tsc --noEmit",
        "type-check:watch": "yarn type-check -- --watch",
        "icons:generate": "node ./scripts/iconsGenerate"
    },
    "dependencies": {
        "@stage-ui/icons": "0.2.0-beta4",
        "@stage-ui/system": "0.2.0-beta4",
        "imask": "6.0.1",
        "moment": "2.29.1",
        "rifm": "0.12.0"
    },
    "peerDependencies": {
        "@babel/runtime": "7.x",
        "@emotion/react": "11.x",
        "react": "16.x",
        "react-dom": "16.x"
    },
    "devDependencies": {
        "@emotion/babel-preset-css-prop": "^11.2.0",
        "@babel/cli": "^7.7.5",
        "@babel/core": "^7.7.5",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-proposal-optional-chaining": "^7.7.5",
        "@babel/plugin-transform-runtime": "^7.7.6",
        "@babel/preset-env": "^7.7.6",
        "@babel/preset-react": "^7.7.4",
        "@babel/preset-typescript": "^7.7.4",
        "csstype": "^3.0.6",
        "eva-icons": "^1.1.2"
    }
}
