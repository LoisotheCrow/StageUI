{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "target": "esnext",
    "outDir": "../../build/core",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "strict": true,
    "types": ["@emotion/react"],
    "declaration": true,
    "emitDeclarationOnly": true
  },
  "exclude": [
    "node_modules"
  ]
}