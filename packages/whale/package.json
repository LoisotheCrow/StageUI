{
  "name": "@flow-ui/whale",
  "author": "ABR Tech",
  "version": "0.1.8",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build": "yarn build:types && yarn build:js && cp package.json ../../build/whale/package.json && cp README.md ../../build/whale/README.md",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel ./ --out-dir ../../build/whale --extensions \".ts,.tsx\" --ignore \"node_modules/\""
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@emotion/babel-preset-css-prop": "^10.0.23",
    "@types/react": "16.9.19",
    "csstype": "^2.6.8",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "@emotion/cache": "^10.0.0",
    "@emotion/core": "^10.0.0",
    "@emotion/css": "^10.0.0",
    "emotion-theming": "^10.0.0",
    "@types/color": "^3.0.1",
    "color": "^3.1.2"
  }
}
