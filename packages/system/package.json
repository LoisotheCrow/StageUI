{
    "name": "@stage-ui/system",
    "author": "Dmitry Beltyukov, Ilya Tricoz",
    "version": "0.1.28",
    "license": "MIT",
    "scripts": {
        "pub": "git pull && yarn build && npm version patch && cd ../../build/system && npm version patch && npm publish --access public && cd .. && rm -rf ./system",
        "build": "yarn build:types && yarn build:js && cp package.json README.md ../../build/system/ && node ../../scripts/buildConfig system",
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "babel ./ --out-dir ../../build/system --extensions \".ts,.tsx\" --ignore \"node_modules/\"",
        "type-check": "tsc --noEmit",
        "type-check:watch": "yarn type-check -- --watch"
    },
    "dependencies": {
        "@emotion/core": "^10.0.27",
        "@emotion/cache": "^10.0.27",
        "emotion-theming": "^10.0.27",
        "@types/color": "^3.0.1",
        "color": "^3.1.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.5",
        "@babel/core": "^7.7.5",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-proposal-optional-chaining": "^7.7.5",
        "@babel/plugin-transform-runtime": "^7.7.6",
        "@babel/preset-env": "^7.7.6",
        "@babel/preset-react": "^7.7.4",
        "@babel/preset-typescript": "^7.7.4",
        "@emotion/babel-preset-css-prop": "^10.0.23",
        "@types/react": "^16.9.23",
        "csstype": "^2.6.8",
        "tslint": "^5.20.1",
        "typescript": "^3.8.3"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    }
}