{
    "name": "@stage-ui/system",
    "author": "Research technology",
    "version": "0.1.43",
    "license": "MIT",
    "scripts": {
        "pub": "git pull && yarn build && npm version patch && cd ../../build/system && npm version patch && yarn publish --access public && cd .. && rm -rf ./system",
        "build": "yarn build:types && yarn build:js && cp package.json README.md ../../build/system/ && node ../../scripts/buildConfig system",
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "babel ./ --out-dir ../../build/system --extensions \".ts,.tsx\" --ignore \"node_modules/\"",
        "type-check": "tsc --noEmit",
        "type-check:watch": "yarn type-check -- --watch"
    },
    "dependencies": {
        "@types/color": "^3.0.1",
        "@babel/runtime": "*",
        "color": "^3.1.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.5",
        "@babel/core": "^7.7.5",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-proposal-optional-chaining": "^7.7.5",
        "@babel/plugin-transform-runtime": "^7.7.6",
        "@babel/preset-env": "^7.7.6",
        "@babel/preset-react": "^7.7.4",
        "@babel/preset-typescript": "^7.7.4",
        "@types/react": "^17.0.0",
        "csstype": "^3.0.6"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*",
        "@emotion/cache": "*",
        "@emotion/react": "*",
        "@emotion/css": "*"
    }
}
