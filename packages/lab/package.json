{
  "name": "@flow-ui/lab",
  "author": "ABR Tech",
  "version": "0.1.23",
  "license": "MIT",
  "scripts": {
    "pub": "git pull && yarn build && npm version patch && cd ../../build/lab && npm version patch && npm publish --access public && cd .. && rm -rf ./lab",
    "build": "yarn build:types && yarn build:js && cp package.json README.md ../../build/lab/ && node ../../scripts/buildConfig lab",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel ./ --out-dir ../../build/lab --extensions \".ts,.tsx\" --ignore \"node_modules/\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "chart.js": "^2.9.3",
    "react-chartjs-2": "^2.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@emotion/babel-preset-css-prop": "^10.0.23",
    "@types/chart.js": "2.9.11",
    "@types/react": "16.9.19",
    "csstype": "^2.6.8",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
