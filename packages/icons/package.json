{
    "name": "@stage-ui/icons",
    "author": "Research technology",
    "version": "0.2.0-beta4",
    "license": "MIT",
    "scripts": {
        "build": "tsc && yarn build:js && cp package.json README.md ../../build/icons/ && node ../../scripts/buildConfig icons",
        "build:types": "tsc --declaration --emitDeclarationOnly",
        "build:js": "babel ./ --out-dir ../../build/icons --extensions \".ts,.tsx\" --ignore \"node_modules/\"",
        "gen": "node ./scripts/generate"
    },
    "devDependencies": {
        "@emotion/babel-preset-css-prop": "^11.2.0"
    },
    "dependencies": {
        "@stage-ui/system": "0.2.0-beta4"
    },
    "peerDependencies": {
        "@emotion/react": "11.x"
    }
}
