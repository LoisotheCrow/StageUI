{
  "name": "@flow-ui/monorepo",
  "author": "ABR Tech",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "themer": "yarn workspace @flow-ui/themer start",
    "playground": "yarn workspace @flow-ui/playground start",
    "playground:a": "yarn workspace @flow-ui/playground analyze",
    "start": "yarn workspace @flow-ui/docs start",
    "test": "yarn workspace @flow-ui/docs test",
    "approve": "yarn workspace @flow-ui/docs approve",
    "gen": "yarn gen:createTypes && yarn gen:convertTypes && yarn gen:generateTypes && yarn gen:fixTypes",
    "gen:createTypes": "cd ./packages/documaker && typedoc -mode file --json ./definitions/original.json ../docs/pages",
    "gen:convertTypes": "cd ./packages/documaker && node ./scripts/convertTypes.js",
    "gen:generateTypes": "cd ./packages/documaker && node ./scripts/generateTypes.js",
    "gen:fixTypes": "cd ./packages/documaker && node ./scripts/fixTypes.js",
    "gen:evaicons": "cd ./packages/core && yarn icons:generate",
    "pub:core": "cd ./packages/core && yarn pub",
    "pub:lab": "cd ./packages/lab && yarn pub",
    "pub:whale": "cd ./packages/whale && yarn pub",
    "gitpush": "git commit . -m 'Update version' && git push",
    "build": "cd ./packages/documaker && yarn build",
    "form": "cd ./packages/documaker && pm2 start --name FlowUI server.js"
  },
  "dependencies": {
    "tslint": "^6.0.0",
    "typescript": "^3.8.3"
  }
}
